#include <iostream>
using namespace std;
int main() {
  int arr[5][5]={{0,0,0,2,0},
   {1,0,0,0,3},
   {0,0,0,0,0},
   {4,0,0,0,0},
   {0,8,0,0,0}};
   int size=0;
   
   for(int i=0;i<5;i++){
       for (int j=0;j<5;j++){
           if(arr[i][j]!=0){
               size++;
           }
       }
   }
   int sp[size][3];
   int row=0;
   for(int i=0;i<5;i++){
       
       for (int j=0;j<5;j++){
           if(arr[i][j]!=0){
               sp[row][0]=i;
               sp[row][1]=j;
               sp[row][2]=arr[i][j];
               row++;
           }
       }
   }
    for(int i=0;i<size;i++){
       for (int j=0;j<3;j++){
           cout<<sp[i][j]<<" ";
           }
           cout<<endl;
       }
       for(int i=0;i<size;i++){
       for (int j=0;j<=0;j++){
          int temp=sp[i][j];
          sp[i][j]=sp[i][j+1];
          sp[i][j+1]=temp;
           }
           
           
       }
       
       for(int i=0;i<size;i++){
        for(int j=0;j<size-1;j++){
        if (sp[j][0]>sp[j+1][0]){
            int temp1=sp[j][0];
            int temp2=sp[j][1];
            int temp3=sp[j][2];
            sp[j][0]=sp[j+1][0];
            sp[j][1]=sp[j+1][1];
            sp[j][2]=sp[j+1][2];
            sp[j+1][0]=temp1;
            sp[j+1][1]=temp2;
            sp[j+1][2]=temp3;
            
        }
       }}
       cout<<endl;
       for(int i=0;i<size;i++){
       for (int j=0;j<3;j++){
           cout<<sp[i][j]<<" ";
           }
           cout<<endl;
       }
       return 0;
   }
