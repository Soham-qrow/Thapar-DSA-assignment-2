#include <iostream>
using namespace std;
int main() {
    int arr[9]={1,3,4,5,6,7,8,9,10};
    int start ,end,s;
    start=0;
    end=8;
    s=6;
    while (start<end){
        int mid=(start+end)/2;
        if (arr[mid]==s){
            cout<<"Index of "<<s<<" is "<<mid;
            break;
        }
        else if (s>arr[mid]){
            start=mid+1;
        }
        else if (s<arr[mid]){
            end=mid-1;
        }
    }

    
}
